{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^5.0.0",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-rewire": "^1.2.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "express-request-language": "^1.1.15",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "^23.6.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.2",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.5.2",
    "redux": "^4.0.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "vcr": "node vcr",
    "start-dev": "cross-env REACT_APP_API_MODE=VCR node scripts/start",
    "chrome-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --env=jsdom"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "vcr/**/*.{js,jsx,mjs}",
      "!<rootDir>/src/*.js",
      "!<rootDir>/src/components/index.js",
      "!<rootDir>/src/components/App/*.js",
      "!<rootDir>/src/services/index.js",
      "!<rootDir>/vcr/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*test.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!@).+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@containers(.*)$": "<rootDir>/src/containers$1",
      "^@helpers(.*)$": "<rootDir>/src/helpers$1",
      "^@services(.*)$": "<rootDir>/src/services$1",
      "^@request(.*)$": "<rootDir>/src/services/request$1",
      "^@status(.*)$": "<rootDir>/src/status$1",
      "^@assets(.*)$": "<rootDir>/src/assets$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/testSetup.js",
    "restoreMocks": true,
    "watchPathIgnorePatterns": [
      "<rootDir>/vcr/testRecordings"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "rewire"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "btoa": "^1.2.1",
    "cors": "^2.8.4",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "node-sass": "^4.9.3",
    "object-hash": "^1.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sass-loader": "^7.1.0",
    "standard": "^11.0.1"
  }
}
